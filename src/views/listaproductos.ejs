    <title>Lista</title>
    
    <h1>Menu de Edici√≥n</h1>

    <section>
        <h2>Tragos</h2> 
        <span>Agregar trago <a href="/nuevo"><i class="fa-solid fa-plus"></i> </a> </span>
        <% if(products.length > 0) %> 
        <ul>
            <% products.forEach(element => { %>
              <h3> <%= element.nombre %> <span> $<%= element.precio%> </span> </h3>  
              <li> <%= element.ingredientes %> <a href="/MenuEdicionMolokai/editar/<%= element.id %> "><i class="fa-solid fa-pen-to-square"></i></a> </li> 

            <% }) %>
        </ul>
    </section>

</head>
